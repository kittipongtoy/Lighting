        <div class="head-office">
            <div class="row justify-content-center">
                <div class="col-lg-8 col-md-7 col-12">
                    <div class="blueBox">
                        <div class="row">
                            <div class="col">
                                @if (lang == "EN")
                                {
                                    <h3>@mainContact?.Title_EN</h3>
                                }
                                else
                                {
                                    <h3>@mainContact?.Title_TH</h3>
                                }

                            </div>
                        </div>
                        <div class="row">
                            <div class="col">
                                <ul class="contact-info mb-0">
                                    <li class="address">
                                        <div class="icon-contact"><i class="fas fa-map-marker-alt"></i></div>
                                        @if (lang == "EN")
                                        {
                                            <p>
                                                <strong> @mainContact?.TitleName_EN</strong>
                                                @mainContact?.Location_EN
                                            </p>
                                        }
                                        else
                                        {
                                            <p>
                                                <strong> @mainContact?.TitleName_TH</strong>
                                                @mainContact?.Location_TH
                                            </p>
                                        }

                                    </li>
                                    <li>
                                        <div class="icon-contact"><i class="fas fa-phone-alt"></i></div>
                                        <p>@mainContact?.PhoneNumber</p>
                                    </li>
                                    <li>
                                        <div class="icon-contact"><i class="fas fa-fax"></i></div>
                                        <p>@mainContact?.OfficePhone</p>
                                    </li>
                                    <li>
                                        <div class="icon-contact"><i class="fas fa-envelope"></i></div>
                                        <ul>
                                            <li>
                                                <p>
                                                    <strong>@mainContact?.TitleEMail1 :</strong>
                                                    <a href="mailto:@mainContact?.EMail1">@mainContact?.EMail1</a>
                                                </p>
                                            </li>
                                            <li>
                                                <p>
                                                    <strong>@mainContact?.TitleEMail2 :</strong>
                                                    <a href="mailto:@mainContact?.EMail2">@mainContact?.EMail2</a>
                                                </p>
                                            </li>
                                        </ul>
                                    </li>
                                    <li class="contactBT">
                                        <a class="buttonB" href="@mainContact?.GoogleMapLink" target="_blank"><i class="fas fa-map-marked-alt"></i>Google map</a>
                                    </li>
                                    <li class="contactBT">
                                        <a class="buttonBD" href="@mainContact?.MoreInfo" target="_blank">More Information</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
