                                        <div class="col-lg-6 d-none d-lg-block">
                                            <a class="img-width imgBD hover-img" data-fancybox href="~/@item?.ImagePath">
                                                <img src="~/@item?.ImagePath">
                                            </a>
                                        </div>
                                        <div class="col-lg-6 col-md-12 col-12">
                                            <div class="contactBox-info">
                                                <div class="row">
                                                    <div class="col">
                                                        <h5>@item?.PlaceName_TH</h5>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col">
                                                        <ul class="contact-info">
                                                            @if (item?.Sub_Factory_Name_TH != null)
                                                            {
                                                                <li class="address">
                                                                    <div class="icon-contact"><i class="fas fa-map-marker-alt"></i></div>
                                                                    @Html.Raw(item?.Sub_Factory_Name_TH)
                                                                </li>
                                                            }
                                                            @if (item?.Location_TH != null)
                                                            {
                                                                <li class="address">
                                                                    <div class="icon-contact"><i class="fas fa-map-marker-alt"></i></div>
                                                                    <p style="word-wrap:break-word;">@item?.Location_TH </p>
                                                                </li>
                                                            }
                                                            @if (item?.CellPhone != null)
                                                            {
                                                                <li>
                                                                    <div class="icon-contact"><i class="fas fa-phone-alt"></i></div>
                                                                    <p>@item?.CellPhone</p>
                                                                </li>
                                                            }
                                                            @if (item?.TelePhone != null)
                                                            {
                                                                <li>
                                                                    <div class="icon-contact"><i class="fas fa-mobile-alt"></i></div>
                                                                    <p>@item?.TelePhone</p>
                                                                </li>
                                                            }
                                                            @if (item?.OfficePhone != null)
                                                            {
                                                                <li>
                                                                    <div class="icon-contact"><i class="fas fa-fax"></i></div>
                                                                    <p>@item?.OfficePhone</p>
                                                                </li>
                                                            }
                                                            @if (item?.Email != null)
                                                            {
                                                                <li>
                                                                    <div class="icon-contact"><i class="fas fa-envelope"></i></div>
                                                                    <a style="word-wrap:break-word;" href="@item?.Email">@Html.Raw(item?.Email)</a>
                                                                </li>
                                                            }
                                                            @if (item?.GoogleMaps_Url != null)
                                                            {
                                                                <li class="map">
                                                                    <a class="buttonB" href="@item?.GoogleMaps_Url" target="_blank"><i class="fas fa-map-marked-alt"></i>Google map</a>
                                                                    @if (lang == "EN")
                                                                    {
                                                                        @if (item?.YouTube_Url_EN != null)
                                                                        {
                                                                            <a class="buttonBD" href="@item?.YouTube_Url_EN" target="_blank"><i class="fab fa-youtube"></i>&nbsp;YouTube</a>
                                                                        }
                                                                    }
                                                                    else
                                                                    {
                                                                        @if (item?.YouTube_Url_TH != null)
                                                                        {
                                                                            <a class="buttonBD" href="@item?.YouTube_Url_TH" target="_blank"><i class="fab fa-youtube"></i>&nbsp;YouTube</a>
                                                                        }
                                                                    }
                                                                </li>
                                                            }
                                                        </ul>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- MAP :: FOR IPAD & MOBILE -->
                                        <div class="col-12 d-block d-lg-none">
                                            <a class="img-width imgBD hover-img" data-fancybox href="~/@item?.ImagePath">
                                                <img src="~/@item?.ImagePath">
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            }
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $(".display-tab > li").click(function () {
                var rel = $(this).attr("rel");
                $('.display-info').hide();
                $('.display-info[rel=' + rel + ']').fadeIn();
                $(".display-tab > li").removeClass("active");
                $(this).addClass("active");
            });
        });
    </script>
}